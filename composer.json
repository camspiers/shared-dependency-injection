{
    "name": "camspiers/silverstripe-dependency-injection",
    "description": "Dependency Injection for SilverStripe 2.4+",
    "authors": [
        {
            "name": "Cam Spiers",
            "email": "camspiers@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Camspiers\\DependencyInjection": "src/"
        }
    },
    "require": {
        "symfony/dependency-injection": "~2.1",
        "symfony/yaml": "~2.1",
        "symfony/config": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "silverstripe/framework": "~2.4",
        "composer/composer": "1.0.0-alpha6"
    },
    "extra": {
        "installer-paths": {
            "sapphire": [ "silverstripe/framework" ]
        }
    }
}
